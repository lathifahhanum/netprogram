@{
    ViewData["Title"] = "Insert Education";
    Layout = "_AdminLayout";
}

<h1>Create</h1>

<h4>Education</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label id="universityName" class="control-label"></label>
                <input id="txtUniversityName" class="form-control" />
                @*<span asp-validation-for="Major" class="text-danger"></span>*@
            </div>
            <div class="form-group">
                <input id="btnSave" type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

@section Scripts{
    <script src="~/js/universityAjax.js"></script>
}

